<div class="well">
  <%= form_for(rate) do |f|%>
    <div class="form-group">
      <%= f.label :user_id %>
      <br>
      <%= f.text_field :user_id, html: {class: "form-control"} %>
    </div>

    <div class="form-group">
      <%= f.label :rate %>
      <br>
      <%= f.text_field :rate, html: {class: "form-control"} %>
    </div>


    <br>
    <%= f.submit(@rate.new_record? ? '新增rate' : '修改rate', class:"btn btn-primary") %>
  <% end %>
</div>